<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name = "QuickSell">

    </style>
    <style name="QuickSell.TextView">

    </style>
    <style name="QuickSell.TextView.HeadLine" parent="@style/TextAppearance.AppCompat.Headline">

    </style>
    <style name="QuickSell.TextView.Large" parent="@style/TextAppearance.AppCompat.Menu">
        <item name="android:ellipsize">end</item>
    </style>
    <style name="QuickSell.TextView.Menu" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:ellipsize">end</item>
    </style>
    <style name="QuickSell.TextView.Warning" parent="@style/TextAppearance.AppCompat.Caption">

    </style>
    <style name="QuickSell.TextView.Toast" parent="@style/TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">8dp</item>
    </style>
    <style name="QuickSell.TextView.Number" parent="@style/TextAppearance.AppCompat.Display1">
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">22dp</item>
    </style>
    <style name="QuickSell.TextView.ListItem" parent="@style/TextAppearance.AppCompat.Display1">

    </style>
    <style name="QuickSell.TextView.ListItem.Title" parent="@style/TextAppearance.MaterialComponents.Subtitle1">

    </style>
    <style name="QuickSell.TextView.ListItem.Body" parent="@style/TextAppearance.MaterialComponents.Body2">

    </style>
    <style name="QuickSell.Divider">
        <item name="android:background">@drawable/divider_solid</item>
    </style>
    <style name="QuickSell.Divider.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="QuickSell.Divider.Vertical">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">0dp</item>
    </style>
    <style name="QuickSell.TextInputLayout" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">32dp</item>
    </style>
    <style name="QuickSell.TextInputEditText">
        <item name="singleLine">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">text</item>
    </style>
    <style name="QuickSell.Button" parent="@style/Widget.MaterialComponents.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginLeft">32dp</item>
        <item name="android:layout_marginRight">32dp</item>
    </style>
    <style name="QuickSell.Button.Outlined" parent="@style/Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginLeft">32dp</item>
        <item name="android:layout_marginRight">32dp</item>
    </style>
    <style name="QuickSell.Button.Transparent" parent="@style/Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">32dp</item>
        <item name="android:layout_marginRight">32dp</item>
    </style>
    <style name="QuickSell.Button.IconButton" parent="@style/Widget.MaterialComponents.Button.OutlinedButton">
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="QuickSell.Button.ContainedButton" parent="@style/Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textSize">22dp</item>
        <item name="iconPadding">0dp</item>
        <item name="android:inset">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
    </style>
    <style name="QuickSell.Toast">
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowEnterAnimation">@anim/slide_in_up</item>
        <item name="android:windowExitAnimation">@anim/slide_out_up</item>
    </style>
    <style name="QuickSell.Overlay">
        <item name="android:windowActivityTransitions">false</item>
    </style>
</resources>